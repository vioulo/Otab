{
    "manifest_version": 2,
    "name": "Otab",
    "version": "24.2.16",
    "browser_specific_settings": {
        "gecko": {
            "id": "Otab@extension.id",
            "strict_min_version": "100"
        }
    },
    "icons": {
        "32": "imgs/icons/icon32.png",
        "48": "imgs/icons/icon48.png",
        "96": "imgs/icons/icon96.png",
        "128": "imgs/icons/icon96.png"
    },
    "chrome_url_overrides": {
        "newtab": "pages/new-tab.html"
    },
    "pages": {
        "settings": "pages/setting.html"
    },
    "permissions": [
        "topSites",
        "bookmarks",
        "storage"
    ]
}